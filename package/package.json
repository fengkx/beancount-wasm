{
  "name": "beancount-wasm",
  "version": "0.3.1",
  "description": "Pyodide-compatible Beancount wheels (v2/v3) and a small installer wrapper",
  "type": "module",
  "license": "GPL-2.0-only",
  "scripts": {
    "build": "tsdown",
    "build:release": "pnpm run build",
    "build:debug": "BEANCOUNT_WASM_PROFILE=debug-symbols pnpm run build",
    "check:profile": "node ../scripts/verify-package-profile.mjs",
    "pack:release": "pnpm run check:profile -- --expect release && pnpm pack",
    "pack:debug": "pnpm run check:profile -- --expect debug-symbols && pnpm pack",
    "prepublishOnly": "pnpm build"
  },
  "exports": {
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./v2": {
      "types": "./dist/v2.d.ts",
      "import": "./dist/v2.js"
    },
    "./v3": {
      "types": "./dist/v3.d.ts",
      "import": "./dist/v3.js"
    },
    "./inline/v2": {
      "types": "./dist/inline/v2.d.ts",
      "import": "./dist/inline/v2.js"
    },
    "./inline/v3": {
      "types": "./dist/inline/v3.d.ts",
      "import": "./dist/inline/v3.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "wheels/",
    "README.md"
  ],
  "keywords": [
    "beancount",
    "pyodide",
    "wasm",
    "webassembly"
  ],
  "devDependencies": {
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengkx/beancount-wasm.git",
    "directory": "package"
  }
}
